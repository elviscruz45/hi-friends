---
const navItems = [
  { name: "Acerca de nosotros", href: "#" },
  { name: "Amigos", href: "#" },
  { name: "Registrate", href: "#" },
];
---

<nav>
  <ul class="item-center flex justify-start gap-8">
    {
      navItems.map((item) => (
        <li>
          <a
            class="text-accent-400 transition-colors duration-300 hover:text-white"
            href={item.href}
          >
            {item.name}
          </a>
        </li>
      ))
    }
  </ul>
</nav>
